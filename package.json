{
  "name": "@sembeacon/website",
  "version": "1.0.0",
  "description": "A project bootstrap for Eleventy sites. With pipelines for PostCSS, JavaScript, and serverless functions",
  "author": "Maxim Van de Wynckel <maxim.van.de.wynckel@vub.be>",
  "contributors": [
    "Beat Signer",
    "Phil Hawksworth"
  ],
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "clean": "rimraf dist",
    "dev": "cross-env ELEVENTY_ENV=dev npx --node-options=\"--experimental-require-module\" eleventy --serve",
    "build": "cross-env ELEVENTY_ENV=prod eleventy",
    "build:docs": "node ./bin/build_docs.js",
    "build:presentations": "node ./bin/build_presentations.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SemBeacon/website"
  },
  "dependencies": {
    "@11ty/eleventy": "3.0.0",
    "@quasibit/eleventy-plugin-sitemap": "^2.2.0",
    "autoprefixer": "^10.4.20",
    "decktape": "^3.14.0",
    "eleventy-favicon": "^1.1.3",
    "eleventy-plugin-seo": "^0.5.2",
    "eleventy-plugin-toc": "^1.1.5",
    "eleventy-sass": "^3.0.0-beta.0",
    "html-minifier": "^4.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-attrs": "^4.2.0",
    "markdown-it-shiki": "^0.9.0",
    "node-fetch": "^3.3.2",
    "nunjucks": "^3.2.4",
    "nunjucks-markdown": "^2.0.1",
    "qrcode": "^1.5.4",
    "reveal.js": "^5.1.0",
    "reveal.js-pointer": "^0.1.4",
    "serve-handler": "^6.1.6",
    "uglify-js": "^3.19.3",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "axios": "^1.7.7",
    "chalk": "^4.x",
    "cross-env": "7.0.3",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0",
    "rimraf": "^6.0.1",
    "unzip-stream": "^0.3.4"
  }
}
