{
  "name": "@sembeacon/website",
  "version": "1.0.0",
  "description": "A project bootstrap for Eleventy sites. With pipelines for PostCSS, JavaScript, and serverless functions",
  "contributors": [
    "Beat Signer",
    "Phil Hawksworth"
  ],
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env ELEVENTY_ENV=dev eleventy --serve",
    "build": "cross-env ELEVENTY_ENV=prod eleventy",
    "build:docs": "node ./bin/build_docs.js",
    "build:presentations": "node ./bin/build_presentations.js",
    "seed": "cross-env ELEVENTY_ENV=seed eleventy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SemBeacon/website"
  },
  "dependencies": {
    "@11ty/eleventy": "2.0.1",
    "@quasibit/eleventy-plugin-sitemap": "^2.2.0",
    "autoprefixer": "^10.4.14",
    "decktape": "^3.10.0",
    "eleventy-favicon": "^1.1.3",
    "eleventy-plugin-seo": "^0.5.2",
    "eleventy-plugin-toc": "^1.1.5",
    "eleventy-sass": "^2.2.3",
    "html-minifier": "^4.0.0",
    "luxon": "^3.3.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-shiki": "^0.9.0",
    "node-fetch": "^3.3.2",
    "nunjucks": "^3.2.4",
    "nunjucks-markdown": "^2.0.1",
    "qrcode": "^1.5.3",
    "reveal.js": "^4.6.1",
    "reveal.js-pointer": "^0.1.4",
    "serve-handler": "^6.1.5",
    "uglify-js": "^3.17.4",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "axios": "^1.4.0",
    "chalk": "^4.x",
    "cross-env": "7.0.3",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "rimraf": "^5.0.1",
    "unzip-stream": "^0.3.1"
  }
}
